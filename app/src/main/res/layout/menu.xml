<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Menu">

    <LinearLayout
        android:id="@+id/linearLayout201"
        android:layout_width="175dp"
        android:layout_height="67dp"
        android:layout_marginTop="8dp"
        android:orientation="horizontal"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/linearLayout20"
        tools:ignore="MissingConstraints">

        <ImageButton
            android:id="@+id/btnofc"
            android:layout_width="73dp"
            android:layout_height="match_parent"
            android:layout_marginLeft="5dp"
            android:adjustViewBounds="true"
            android:background="@drawable/btnblack"
            android:fontFamily="@font/poppins_medium"
            android:gravity="bottom|center"
            android:padding="10dp"
            android:scaleType="fitCenter"
            android:src="@drawable/usererr"
            android:text="Office"
            android:textColor="#FFFFFF"
            android:textSize="10dp" />

        <ImageButton
            android:id="@+id/btnhealth"
            android:layout_width="73dp"
            android:layout_height="match_parent"
            android:layout_marginLeft="15dp"
            android:adjustViewBounds="true"
            android:background="@drawable/btnpurple"
            android:gravity="bottom|center"
            android:padding="10dp"
            android:scaleType="fitCenter"
            android:src="@drawable/cora"
            android:text="Healthy"
            android:textColor="#FFFFFF"
            android:textSize="10dp" />


    </LinearLayout>

    <LinearLayout
        android:id="@+id/linearLayout2"
        android:layout_width="442dp"
        android:layout_height="50dp"
        android:background="@drawable/navegacion_barra"
        android:orientation="horizontal"
        app:layout_constraintBottom_toTopOf="@+id/linearLayout7"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.451"
        app:layout_constraintStart_toStartOf="parent"
        tools:ignore="MissingConstraints">

        <Button
            android:id="@+id/home"
            android:layout_width="40dp"
            android:layout_height="40dp"
            android:layout_marginLeft="30dp"
            android:layout_marginTop="10dp"
            android:background="@drawable/casa"
            android:textSize="32dp"

            />

        <Button
            android:id="@+id/lupa"
            android:layout_width="40dp"
            android:layout_height="40dp"
            android:layout_marginLeft="70dp"
            android:layout_marginTop="10dp"
            android:background="@drawable/lupa"
            android:textSize="32dp"

            />

        <Button
            android:id="@+id/cart"
            android:layout_width="40dp"
            android:layout_height="40dp"
            android:layout_marginLeft="70dp"
            android:layout_marginTop="10dp"
            android:background="@drawable/carrito_de_compras"
            android:textSize="32dp"

            />

        <Button
            android:id="@+id/account"
            android:layout_width="40dp"
            android:layout_height="40dp"
            android:layout_marginLeft="70dp"
            android:layout_marginTop="10dp"
            android:background="@drawable/usuario"
            android:textSize="32dp"

            />

        <TextView
            android:id="@+id/textView6"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="32dp"
            android:fontFamily="@font/poppins_semiblod"
            android:text="Home"
            android:textColor="@color/black" />


    </LinearLayout>

    <ListView
        android:id="@+id/lista"
        android:layout_width="366dp"
        android:layout_height="186dp"
        app:layout_constraintBottom_toTopOf="@+id/linearLayout2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.488"

        app:layout_constraintStart_toStartOf="parent"

        app:layout_constraintTop_toBottomOf="@+id/horizontalScrollView"
        app:layout_constraintVertical_bias="0.675" />

    <View
        android:id="@+id/view22"
        android:layout_width="212dp"
        android:layout_height="137dp"
        android:layout_marginTop="48dp"
        android:background="@drawable/logo"
        app:layout_constraintEnd_toStartOf="@+id/linearLayout201"
        app:layout_constraintHorizontal_bias="0.6"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/delivery"
        android:layout_width="313dp"
        android:layout_height="19dp"
        android:layout_marginTop="16dp"
        android:background="@drawable/searchview"
        android:fontFamily="@font/poppins_semiblod"
        android:gravity="left"
        android:text=" delivery address"
        android:textSize="12dp"
        app:layout_constraintBottom_toTopOf="@+id/view22"
        app:layout_constraintEnd_toStartOf="@+id/profile_pic2"
        app:layout_constraintHorizontal_bias="0.297"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0" />

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="14dp"
        android:layout_height="15dp"
        android:layout_marginTop="4dp"
        android:layout_marginEnd="4dp"
        android:alpha="0.51"
        app:layout_constraintBottom_toBottomOf="@+id/delivery"
        app:layout_constraintEnd_toEndOf="@+id/delivery"
        app:layout_constraintTop_toTopOf="@+id/delivery"
        app:layout_constraintVertical_bias="0.0"
        app:srcCompat="@drawable/navigation" />

    <TextView
        android:id="@+id/txtDeli"
        android:layout_width="88dp"
        android:layout_height="38dp"
        android:fontFamily="@font/poppins_medium"
        android:text="Deli"
        android:textColor="#FFFFFF"
        android:textSize="35sp"
        app:layout_constraintBottom_toBottomOf="@+id/view22"
        app:layout_constraintEnd_toEndOf="@+id/view22"
        app:layout_constraintHorizontal_bias="0.491"
        app:layout_constraintStart_toStartOf="@+id/view22"
        app:layout_constraintTop_toTopOf="@+id/view22"
        app:layout_constraintVertical_bias="0.191" />

    <TextView
        android:id="@+id/txtFood"
        android:layout_width="121dp"
        android:layout_height="44dp"
        android:fontFamily="@font/poppins_medium"
        android:text=" Food"
        android:textColor="#CC00FF"
        android:textSize="40sp"
        app:layout_constraintBottom_toBottomOf="@+id/view22"
        app:layout_constraintEnd_toEndOf="@+id/view22"
        app:layout_constraintHorizontal_bias="0.467"
        app:layout_constraintStart_toStartOf="@+id/view22"
        app:layout_constraintTop_toBottomOf="@+id/txtDeli"
        app:layout_constraintVertical_bias="0.0" />

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="12dp"
        android:layout_height="13dp"
        android:layout_marginTop="15dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.19"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/view22"
        app:srcCompat="@drawable/pp" />

    <LinearLayout
        android:id="@+id/linearLayout20"
        android:layout_width="175dp"
        android:layout_height="67dp"
        android:layout_marginTop="16dp"
        android:orientation="horizontal"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toEndOf="@+id/view22"
        app:layout_constraintTop_toBottomOf="@+id/delivery"
        tools:ignore="MissingConstraints">

        <ImageButton
            android:id="@+id/buttonexc"
            android:layout_width="73dp"
            android:layout_height="match_parent"
            android:layout_marginLeft="5dp"
            android:adjustViewBounds="true"
            android:background="@drawable/btnblack"
            android:fontFamily="@font/poppins_medium"
            android:padding="10dp"
            android:scaleType="fitCenter"
            android:src="@drawable/diamante"
            android:text="Exclusive"
            android:textColor="#FFFFFF"
            android:textSize="10dp" />

        <ImageButton
            android:id="@+id/buttongaming"
            android:layout_width="73dp"
            android:layout_height="match_parent"
            android:layout_marginLeft="15dp"
            android:adjustViewBounds="true"
            android:background="@drawable/btnpurple"
            android:fontFamily="@font/poppins_medium"
            android:gravity="bottom|center"
            android:padding="10dp"
            android:scaleType="fitCenter"
            android:src="@drawable/gam"
            android:text="Gaming"
            android:textColor="#FFFFFF"
            android:textSize="10dp" />

    </LinearLayout>

    <de.hdodenhof.circleimageview.CircleImageView
        android:id="@+id/profile_pic2"
        android:layout_width="35dp"
        android:layout_height="29dp"
        android:src="@drawable/perfil_hombre_basico_738242395"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.957"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.008"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="90dp"
        android:layout_height="17dp"
        android:layout_marginStart="8dp"
        android:fontFamily="@font/poppins_medium"
        android:gravity="center"
        android:text="Mauricio Fernandez"
        android:textColor="@color/black"
        android:textSize="6dp"
        app:layout_constraintBottom_toTopOf="@+id/linearLayout20"
        app:layout_constraintStart_toEndOf="@+id/delivery"
        app:layout_constraintTop_toBottomOf="@+id/profile_pic2"
        app:layout_constraintVertical_bias="1.0" />

    <HorizontalScrollView
        android:id="@+id/horizontalScrollView"

        android:layout_width="345dp"
        android:layout_height="118dp"
        android:layout_marginTop="16dp"

        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.333"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/view22">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">

            <androidx.cardview.widget.CardView
                android:layout_width="200dp"
                android:layout_height="150dp"
                android:layout_marginLeft="2dp">

                <ImageView
                    android:layout_width="match_parent"
                    android:layout_height="112dp"
                    android:src="@drawable/kfccombo" />

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="80dp"
                    android:alpha="0.6"
                    android:background="@drawable/navegacion_barra"
                    android:fontFamily="@font/poppins_medium"
                    android:gravity="center"
                    android:text="2X1 en Combo"
                    android:textColor="@color/black"
                    android:textSize="20dp" />

            </androidx.cardview.widget.CardView>

            <androidx.cardview.widget.CardView
                android:layout_width="200dp"
                android:layout_height="150dp"
                android:layout_marginLeft="20dp"

                android:layout_marginRight="33dp">

                <ImageView
                    android:layout_width="match_parent"
                    android:layout_height="112dp"
                    android:scaleType="fitXY"
                    android:src="@drawable/hutcombo" />

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="80dp"
                    android:alpha="0.5"
                    android:background="@drawable/navegacion_barra"
                    android:fontFamily="@font/poppins_medium"
                    android:gravity="center"
                    android:text="Familiar 30%"
                    android:textColor="@color/black"
                    android:textSize="20dp" />

            </androidx.cardview.widget.CardView>

            <androidx.cardview.widget.CardView
                android:layout_width="200dp"
                android:layout_height="150dp"
                android:layout_marginLeft="0dp">

                <ImageView
                    android:layout_width="match_parent"
                    android:layout_height="112dp"
                    android:src="@drawable/macombo" />

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="85dp"
                    android:alpha="0.6"
                    android:background="@drawable/navegacion_barra"
                    android:fontFamily="@font/poppins_medium"
                    android:gravity="center"
                    android:text="Solo Por Hoy 40%"
                    android:textColor="@color/black"
                    android:textSize="17.5dp" />

            </androidx.cardview.widget.CardView>

            <androidx.cardview.widget.CardView
                android:layout_width="200dp"
                android:layout_height="150dp"
                android:layout_marginLeft="20dp">

                <ImageView
                    android:layout_width="match_parent"
                    android:layout_height="112dp"
                    android:scaleType="fitXY"
                    android:src="@drawable/sushicombo" />

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="80dp"
                    android:alpha="0.5"
                    android:background="@drawable/navegacion_barra"
                    android:fontFamily="@font/poppins_medium"
                    android:gravity="center"
                    android:text="Sushiweek"
                    android:textColor="@color/black"
                    android:textSize="20dp" />

            </androidx.cardview.widget.CardView>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical" /><![CDATA[



                    >

            ]]>
        </LinearLayout>


    </HorizontalScrollView>

    <LinearLayout
        android:background="@drawable/navegacion_barra"
        android:id="@+id/linearLayout7"
        android:layout_width="413dp"
        android:layout_height="30dp"
        android:orientation="horizontal"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent">

        <TextView
            android:gravity="center"
            android:layout_marginLeft="17dp"
            android:fontFamily="@font/poppins_medium"
            android:id="@+id/textView4"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:text="Home"
            android:textColor="@color/black"/>
        <TextView
            android:gravity="center"
            android:layout_marginLeft="60dp"
            android:fontFamily="@font/poppins_medium"
            android:id="@+id/textView44"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:text="Search"
            android:textColor="@color/black"/>
        <TextView
            android:gravity="center"
            android:layout_marginLeft="75dp"
            android:fontFamily="@font/poppins_medium"
            android:id="@+id/textView4422"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:text="Cart"
            android:textColor="@color/black"/>
        <TextView
            android:gravity="center"
            android:layout_marginLeft="60dp"
            android:fontFamily="@font/poppins_medium"
            android:id="@+id/textView441"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:text="Account"
            android:textColor="@color/black"/>
    </LinearLayout>


</androidx.constraintlayout.widget.ConstraintLayout>